{
  "name": "achiever",
  "version": "1.0.0",
  "description": "A tool to achieve :)",
  "main": "serve.js",
  "scripts": {
    "prebuild": "npm run check",
    "prestart": "npm run check",
    "prewatch": "npm run check",
    "check": "check-node-version --print --node 8 --npm 5 && check-dependencies",
    "lint": "eslint --ext .jsx,.js .",
    "build": "webpack --config webpack.prod.js",
    "start": "node server/index.js",
    "start:debug": "DEBUG=true npm start",
    "start:debug-win": "set DEBUG=true && npm start",
    "quickstart": "node server/index.js",
    "watch": "nodemon server/index.js",
    "watch:debug": "DEBUG=true npm run watch",
    "watch:debug-win": "set DEBUG=true && npm run watch",
    "test": "jest",
    "docker:build": "docker build -t achiever:latest .",
    "prepm2:start": "npm run build",
    "pm2:start": "NODE_ENV=production pm2 start achiever.config.js",
    "pm2:stop": "pm2 stop achiever.config.js",
    "pm2:logs": "pm2 logs achiever",
    "pm2:list": "pm2 list achiever.config.js"
  },
  "author": {
    "name": "Marcos Sandrini",
    "email": "msandrinidesign@gmail.com"
  },
  "contributors": [
    {
      "name": "Everton Nikolas de Oliveira",
      "email": "enikolas.oliveira@gmail.com"
    },
    {
      "name": "Henrique Noronha Facioli",
      "email": "henrique.facioli@students.ic.unicamp.br"
    }
  ],
  "license": "ISC",
  "precommit": [
    "lint"
  ],
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "check-dependencies": "^1.1.0",
    "check-node-version": "^3.2.0",
    "clean-webpack-plugin": "^0.1.18",
    "compression-webpack-plugin": "^1.1.6",
    "css-loader": "^0.28.7",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.6.1",
    "html-webpack-plugin": "^2.30.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.1.4",
    "jest-transform-graphql": "^2.1.0",
    "nodemon": "^1.14.12",
    "pre-commit": "^1.2.2",
    "react-test-renderer": "^16.2.0",
    "style-loader": "^1.2.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^2.0.4",
    "webpack-hot-middleware": "^2.21.0",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.1.7",
    "apollo-client": "^2.2.2",
    "apollo-link": "^1.0.7",
    "apollo-link-http": "^1.3.2",
    "apollo-server-express": "^1.3.0",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.2",
    "chalk": "^2.3.0",
    "cheerio": "^1.0.0-rc.2",
    "co": "^4.6.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "dotenv": "^5.0.0",
    "express": "^4.16.2",
    "graphql": "^0.13.0",
    "graphql-tag": "^2.6.1",
    "graphql-tools": "^2.20.0",
    "jwt-simple": "^0.5.1",
    "md5": "^2.2.1",
    "mockserver": "^1.6.2",
    "moment": "^2.19.4",
    "moment-timezone": "^0.5.14",
    "morgan": "^1.9.0",
    "node-persist": "^2.1.0",
    "node-storage": "0.0.7",
    "pm2": "^2.9.3",
    "prompt": "^1.0.0",
    "prop-types": "^15.6.0",
    "react": "^16.1.0",
    "react-apollo": "^2.0.4",
    "react-datepicker": "^1.1.0",
    "react-dom": "^16.1.0",
    "react-router-dom": "^4.2.2",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "time-duration": "^1.1.4",
    "tough-cookie": "^2.3.3",
    "whatwg-fetch": "^2.0.3"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "setupFiles": [
      "<rootDir>/testSetup.js",
      "<rootDir>/__mocks__/localStorage.js"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!time-duration)"
    ],
    "transform": {
      "\\.(gql|graphql)$": "jest-transform-graphql",
      "\\.*": "babel-jest"
    },
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ],
    "moduleNameMapper": {
      "\\.(css|less|styl|sass)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    }
  }
}
